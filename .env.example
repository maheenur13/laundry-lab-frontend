# API Configuration
# Copy this file to .env and update with your values

# Development API URL (your local machine IP for physical device testing)
API_URL_DEV=http://192.168.x.x:3000/api/v1

# Production API URL
API_URL_PROD=https://your-production-api.com/api/v1

# For Android Emulator (uses special IP to access host machine)
API_URL_ANDROID_EMU=http://10.0.2.2:3000/api/v1
