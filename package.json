{
  "name": "laundrybd-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:dev-a": "eas build --profile development --platform android",
    "build:prod-a": "eas build --profile production --platform android",
    "build:prod-ios": "eas build --profile production --platform ios",
    "set:version": "eas build:version:set",
    "submit:ios": "eas submit -p ios --latest",
    "update:prod": "APP_ENV=production eas update --channel production",
    "ts:check": "tsc",
    "apk:build": "eas build -p android --profile preview",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@gluestack-style/react": "^1.0.57",
